{{ form_start(form) }}
    <div class="row">
        <div class="col">
            {{ form_row(form.title) }}
        </div>
        <div class="col">
            {{ form_row(form.starts) }}
        </div>
        <div class="col">
            {{ form_row(form.ends) }}
        </div>
    </div>
    <div class="row">
        <div class="form-group col">
            {{ form_row(form.url) }}
        </div>

        <div class="form-group col">
            {{ form_row(form.source) }}
        </div>
        <div class="form-group col">
            {{ form_row(form.venueName) }}
        </div>
    </div>
{{ form_end(form) }}

